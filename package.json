{
  "name": "protocols",
  "version": "0.1.0",
  "description": "Syntropy protocols documentation",
  "main": "index.js",
  "private": true,
  "scripts": {
    "serve": "serve",
    "serve:sdn": "serve build/sdn/",
    "serve:platform": "serve build/platform",
    "build": "concurrently \"npm run build:platform\" \"npm run build:sdn\"",
    "build:platform": "ag platform-protocol.yml @asyncapi/html-template -o ./public/platform -p singleFile=true --force-write --debug",
    "build:sdn": "ag sdn-protocol.yml @asyncapi/html-template -o ./public/sdn -p singleFile=true --force-write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyntropyNet/protocols.git"
  },
  "author": "Syntropy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SyntropyNet/protocols/issues"
  },
  "homepage": "https://github.com/SyntropyNet/protocols#readme",
  "devDependencies": {
    "@asyncapi/generator": "^1.8.9",
    "@asyncapi/html-template": "^0.23.7",
    "concurrently": "^6.2.1",
    "serve": "^12.0.1"
  },
  "dependencies": {}
}
